<style type="text/css">
	.header-col{
		padding: 0px;
	}
	.admin-dash-cont{
		min-height: 550px;
		background-color: white;
		margin-top: 15px;
	}
	.lookup-col{
		text-align: center;
		margin-top: 30px;
	}
	#file{
	    border: 1px solid #ccc;
	    height: 34px;
	    padding-left: 15px;
	    padding-top: 5px;
	    width: 200px;
	}
</style>
<div class="container admin-dash-cont">
	<div class="row">
	    <div class="col-sm-12 header-col">
	      <div class="modal-header">
	        <h3 class="modal-title modalh3" id="loginModalLabel">Admin Dashboard</h3>
	      </div>
	    </div>
    </div>
    <% if flash[:errors] %>
	    <div class="row">
	    	<div class="col-sm-6">
	    		<h5><%= flash[:errors] %></h5>
	    	</div>
	    </div>
    <% end %>
    <div class="row">
    	<div class="col-sm-4 lookup-col">
    		<h4 class="lookup-header">User Lookup</h4>
			<form class="navbar-form" role="search" method="post" action="/users/search">
			<input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">
			<div class="input-group">
				<input type="text" class="form-control" placeholder="ID, Email, Name, Etc.." name="search" id="srch-term">
				<div class="input-group-btn">
					<button class="btn btn-default" type="submit"><i class="glyphicon glyphicon-search"></i></button>
				</div>
			</div>
			</form>
    	</div>
    	<div class="col-sm-4 lookup-col">
    		<h4 class="lookup-header">Commission Lookup</h4>
			<form class="navbar-form" role="search" method="post" action="/commissions/search">
			<input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">
			<div class="input-group">
				<input type="text" class="form-control" placeholder="ID, Email, Name, Etc.." name="search" id="srch-term">
				<div class="input-group-btn">
					<button class="btn btn-default" type="submit"><i class="glyphicon glyphicon-search"></i></button>
				</div>
			</div>
			</form>
    	</div>
    	<div class="col-sm-4 lookup-col">
    		<h4 class="lookup-header">Blogs</h4>
    		<a href="/blogs/new">New</a> | 
    		<a href="/blogs/edit">Edit</a>
    	</div>
    </div>
    <div class="row">
    	<div class="col-sm-4 lookup-col">
    		<h4 class="lookup-header">Import Users</h4>
			<form class="navbar-form" role="search" method="post" action="/import_users" enctype="multipart/form-data">
			<input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">
			<div class="input-group">
		  		<input type="file" name="file" id="file" class="form-control-file file" required/>
				<div class="input-group-btn">
					<button class="btn btn-default" type="submit"><i class="glyphicon glyphicon-import"></i></button>
				</div>
			</div>
			</form>
			<% if flash[:imported] %>
				<h5><%= flash[:imported] %></h5>
			<% end %>
    	</div>
<!--     	<div class="col-sm-4 lookup-col">
    		<h4 class="lookup-header">Import Commissions</h4>
			<form class="navbar-form" role="search" method="post" action="/import_commissions" enctype="multipart/form-data">
			<input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">
			<div class="input-group">
		  		<input type="file" name="file" id="file" class="form-control-file file" required/>
				<div class="input-group-btn">
					<button class="btn btn-default" type="submit"><i class="glyphicon glyphicon-import"></i></button>
				</div>
			</div>
			</form>
    	</div> -->
    	<div class="col-sm-4 lookup-col">
    		<h4 class="lookup-header">Unprocessed Commissions</h4>
			<form class="navbar-form" role="search" method="post" action="/commissions/unprocessed_search">
			<input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">
			<div class="input-group">
				<!-- <input type="text" class="form-control" placeholder="ID, Email, Name, Etc.." name="search" id="srch-term"> -->
				<div class="input-group-btn">
					<button class="btn btn-default" type="submit"><i class="glyphicon glyphicon-search"></i></button>
				</div>
			</div>
			</form>
    	</div>
    	<div class="col-sm-4 lookup-col">
    		<a href="admins_active_users"><h5>Active Users</h5></a>
    		<a href="admins_today_users"><h5>Users Updated/Created Today</h5></a>
    	</div>
    </div>
</div>